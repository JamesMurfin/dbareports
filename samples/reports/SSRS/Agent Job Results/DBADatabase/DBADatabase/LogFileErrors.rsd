<?xml version="1.0" encoding="utf-8"?>
<SharedDataSet xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/shareddatasetdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <DataSet Name="">
    <Query>
      <DataSourceReference>DBA Database</DataSourceReference>
      <CommandText>SELECT [FileName]
      ,[ErrorMsg]
      ,[Line]
	  ,[Matches]
  FROM [dbareports].[Info].[LogFileErrorMessages]</CommandText>
    </Query>
    <Fields>
      <Field Name="FileName">
        <DataField>FileName</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="ErrorMsg">
        <DataField>ErrorMsg</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
      <Field Name="Line">
        <DataField>Line</DataField>
        <rd:TypeName>System.Int32</rd:TypeName>
      </Field>
      <Field Name="Matches">
        <DataField>Matches</DataField>
        <rd:TypeName>System.String</rd:TypeName>
      </Field>
    </Fields>
  </DataSet>
</SharedDataSet>